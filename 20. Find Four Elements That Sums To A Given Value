#include <bits/stdc++.h>

string fourSum(vector<int> arr, int target, int n) {
    // Write your code here.
    sort(arr.begin(),arr.end());
    for(int i=0;i<n-3;i++)
    {
        for(int j=i+1;j<n-2;j++)
        {
            int k = j+1;
            int l = n-1;
            int val = target - arr[i]-arr[j];
            while (k < l) 
            {
              if (arr[k] + arr[l] == val)
                return "Yes";
              else if(arr[k]+arr[l]>val)
                l--;
              else
                k++;
            }
        }
    }
    return "No";
}

// TC :- O(N^3)
// SC :- O(1)
